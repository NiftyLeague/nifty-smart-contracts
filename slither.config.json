{
  "compile_force_framework": "hardhat",
  "filter_paths": "node_modules,contracts/mocks,contracts/external",
  "hardhat_artifacts_directory": "./artifacts",
  "hardhat_cache_directory": "./cache",
  "hardhat_ignore_compile": false,
  "solc_remaps": [
    "@openzeppelin/=./node_modules/@openzeppelin/",
    "hardhat/=./node_modules/hardhat/",
    "@chainlink/=./node_modules/@chainlink/"
  ],
  "detectors_to_exclude": "solc-version,naming-convention,similar-names,dead-code,too-many-digits",
  "show_ignored_findings": false,
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false
}
